<div class="container-fluid">
  <div class="row">
    <%= form_for(@tr_jurnal2) do |tr_jurnal2| %>


        <div class="form-group">
          <%= tr_jurnal2.label :Tanggal %>
          <%= tr_jurnal2.text_field :tanggal, class:'form-control', type: 'date', placeholder: '',required: true %>
        </div>

        
        <div class="form-group">
          <%= tr_jurnal2.label :Judul %>
          <%= tr_jurnal2.text_field :judul, class:'form-control', type: 'text', placeholder: '',required: true %>
        </div>
        <div class="form-group">
          <%= tr_jurnal2.label :Pengarang %>
          <%= tr_jurnal2.text_field :pengarang, class:'form-control', type: 'text', placeholder: '',required: true %>
        </div>
        <div class="form-group">
          <%= tr_jurnal2.label :Resume %>
          <%= tr_jurnal2.text_field :resume, class:'form-control', type: 'text', placeholder: '',required: true %>
        </div>
        
        <%= tr_jurnal2.hidden_field :nama, value: current_ms_user.name %>
        <%= tr_jurnal2.hidden_field :status, value: "Menunggu Di Terima" %>
        <div style="float: left">
  <%= link_to 'Back', '/tr_jurnal2s', class: 'btn btn-primary' %>
        </div>
        <div style="float: right">
  <%= tr_jurnal2.submit :Save, class:'btn btn-success' %>
</div>
    <% end %>
  </div>
</div>