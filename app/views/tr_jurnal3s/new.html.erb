<div class="container-fluid">
  <div class="row">
    <%= form_for(@tr_jurnal3) do |tr_jurnal3| %>


        <div class="form-group">
          <%= tr_jurnal3.label :Tanggal %>
          <%= tr_jurnal3.text_field :tanggal, class:'form-control', type: 'date', placeholder: '',required: true %>
        </div>

        
        <div class="form-group">
          <%= tr_jurnal3.label :Jumlah_Text %>
          <%= tr_jurnal3.text_field :jumlah_text, class:'form-control', type: 'text', placeholder: '',required: true %>
        </div>
        <div class="form-group">
          <%= tr_jurnal3.label :Nilai %>
          <%= tr_jurnal3.text_field :nilai, class:'form-control', type: 'text', placeholder: '',required: true %>
        </div>
        
        <%= tr_jurnal3.hidden_field :nama, value: current_ms_user.name %>
        <%= tr_jurnal3.hidden_field :status, value: "Menunggu Di Terima" %>
        <div style="float: left">
  <%= link_to 'Back', '/tr_jurnal3s', class: 'btn btn-primary' %>
        </div>
        <div style="float: right">
  <%= tr_jurnal3.submit :Save, class:'btn btn-success' %>
</div>
    <% end %>
  </div>
</div>