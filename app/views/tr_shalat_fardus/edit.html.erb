<div class="container-fluid">
  <div class="row">
    <%= form_for(@tr_shalat_fardus) do |tr_shalat_fardu| %>


        <div class="form-group">
          <%= tr_shalat_fardu.label :Tanggal %>
          <%= tr_shalat_fardu.text_field :tanggal, class:'form-control', type: 'date', placeholder: '' %>
        </div>

        <%= tr_shalat_fardu.hidden_field :nama, value: current_ms_user.name %>
        <%= tr_shalat_fardu.hidden_field :status, value: "Menunggu Di Terima" %>
        <div style="float: left">
  <%= link_to 'Back', '/tr_shalat_fardus', class: 'btn btn-primary' %>
        </div>
        <div style="float: right">
  <%= tr_shalat_fardu.submit :Save, class:'btn btn-success' %>
</div>
    <% end %>
  </div>
</div>