<div class="container-fluid">
  <div class="row">
    <%= form_for(@tr_ceramahs) do |tr_ceramah| %>


        <div class="form-group">
          <%= tr_ceramah.label :Tanggal %>
          <%= tr_ceramah.text_field :tanggal, class:'form-control', type: 'date', placeholder: '' %>
        </div>

        <div class="form-group">
          <%= tr_ceramah.label :Tempat %>
          <%= tr_ceramah.text_field :tempat, class:'form-control', type: 'text', placeholder: '' %>
        </div>

        <div class="form-group">
          <%= tr_ceramah.label :Judul %>
          <%= tr_ceramah.text_field :judul, class:'form-control', type: 'text', placeholder: '' %>
        </div>

        <div class="form-group">
          <%= tr_ceramah.label :Pemateri %>
          <%= tr_ceramah.text_field :pemateri, class:'form-control', type: 'text', placeholder: '' %>
        </div>

        <div class="form-group">
          <%= tr_ceramah.label :Kesimpulan %>
          <%= tr_ceramah.text_field :kesimpulan, class:'form-control', type: 'text', placeholder: '' %>
        </div>

        <%= tr_ceramah.hidden_field :nama, value: current_ms_user.name %>
        <%= tr_ceramah.hidden_field :status, value: "Menunggu Di Terima" %>
        <div style="float: left">
  <%= link_to 'Back', '/tr_ceramahs', class: 'btn btn-primary' %>
        </div>
       <div style="float: right">
  <%= tr_ceramah.submit :Save, class:'btn btn-success' %>
</div>
    <% end %>
  </div>
</div>