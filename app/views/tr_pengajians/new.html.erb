<div class="container-fluid">
  <div class="row">
    <%= form_for(@tr_pengajian) do |tr_pengajian| %>
        <div class="form-group">
          <%= tr_pengajian.label :Tanggal %>
          <%= tr_pengajian.text_field :tanggal, class:'form-control', type: 'date', placeholder: '', required: true %>
        </div>

        <div class="form-group">
          <%= tr_pengajian.label :Judul %>
          <%= tr_pengajian.text_field :judul, class:'form-control', type: 'text', placeholder: '', required: true %>
        </div>

         <div class="form-group">
          <%= tr_pengajian.label :Kesimpulan %>
          <%= tr_pengajian.text_field :kesimpulan, class:'form-control', type: 'text', placeholder: '', required: true %>
        </div>
        <%= tr_pengajian.hidden_field :nama, value: current_ms_user.name %>
        <%= tr_pengajian.hidden_field :status, value: "Menunggu Di Terima" %>
        <div style="float: left">
  <%= link_to 'Back', '/tr_pengajians', class: 'btn btn-primary' %>
        </div>
       <div style="float: right">
  <%= tr_pengajian.submit :Save, class:'btn btn-success' %>
</div>
    <% end %>
  </div>
</div>