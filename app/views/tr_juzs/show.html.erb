<div class="container-fluid">
  <div class="row">
    <%= form_for(@tr_juz) do |tr_juz| %>


        <div class="form-group">
          <%= tr_juz.label :Tanggal %>
          <%= tr_juz.text_field :tanggal, class:'form-control', type: 'date', placeholder: '', disabled: true %>
        </div>

        <div class="form-group">
          <%= tr_juz.label :Ayat %>
         <%= tr_juz.text_field :ayat, class:'form-control', type: 'text', placeholder: '', disabled: true%>
        </div>
        <%= tr_juz.hidden_field :nama, value: current_ms_user.name %>
        <%= tr_juz.hidden_field :status, value: "Menunggu Di Terima" %>
        <div style="float: left">
  <%= link_to 'Back', '/tr_juzs', class: 'btn btn-primary' %>
        </div>
        
    <% end %>
  </div>
</div>